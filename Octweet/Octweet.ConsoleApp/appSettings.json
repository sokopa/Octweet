{
	"Serilog": {
		"Using": [ "Serilog.Sinks.Console" ],
		"MinimumLevel": "Debug",
		"WriteTo": [
			{ "Name": "Console" }
		],
		"Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
		"Properties": {
			"Application":  "Octweet.ConsoleApp"
		}
	},
	"ConnectionStrings": {
		"OctweetDB": "Data Source=.;Initial Catalog=OctweetDB;User ID=sa;Password=SuperSecretPassword@123;MultipleActiveResultSets=True"
	},
	"Twitter": {
		"ApiKey": "<secret>",
		"ApiSecret": "<secret>",
		"BearerToken": "<secret>",
		"AccessToken": "<secret>",
		"AccessTokenSecret": "<secret>",
		"Query": "#frontpages",
		"PollingPeriodInSeconds": 60
	},
	"Google": {
		"VisionCredentialsPath": "<secret>"
	}
}